	</parameter>
	<parameter name="from_date" class="java.sql.Timestamp"/>
	<parameter name="to_date" class="java.sql.Timestamp"/>
	<queryString>
		<![CDATA[SELECT sol.name as name,sum(sol.product_uom_qty) as qty,sum(sol.price_unit) as unit_price,
	  sum(price_unit * sol.product_uom_qty+ get_tax_amount_for_a_unit(sol.id)*sol.product_uom_qty)
FROM sale_order_line sol
WHERE product_id IN(
  SELECT pp.id
  FROM product_product pp
    INNER JOIN product_template pt ON pp.product_tmpl_id = pt.id
  WHERE pt.categ_id IN (select category_id from syncjob_chargetype_category_mapping
                          WHERE chargetype_name = 'Investigations'))
      AND (sol.create_date BETWEEN  $P{from_date} AND $P{to_date})
      AND (sol.state = 'confirmed' or sol.state ='done')
      and sol.order_partner_id=$P{patient_id}
GROUP BY sol.name,sol.price_unit,sol.product_id;]]>
	</queryString>
